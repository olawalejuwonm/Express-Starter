{
  "name": "ExpressStarter",
  "version": "1.0.0",
  "main": "./bin/www",
  "scripts": {
    "start:dev": "nodemon --exec ts-node ./src/bin/www.ts",
    "start:commit": "ts-node ./src/bin/www.ts",
    "dev": "concurrently -k -p \"[{name}]\" -n \"server\" -c \"blue.bold,green.bold\" \"tsc --watch\" \"nodemon  dist/index.js\"",
    "dev:fast": "nodemon --exec ./node_modules/.bin/ts-node --transpile-only  ./src/bin/www.ts",
    "dev:sucrase": "nodemon --exec ./node_modules/.bin/ts-node --transpiler sucrase/ts-node-plugin ./src/bin/www.ts --experimentalDecorators",
    "start": "ts-node --transpile-only ./src/bin/www.ts",
    "buildproj": "rm -rf dist && tsc",
    "script": "./node_modules/.bin/ts-node",
    "run": "ts-node --transpile-only",
    "lint": "eslint --fix",
    "startproj": "node dist/index.js",
    "prepare": "husky install"
  },
  "license": "MIT",
  "dependencies": {
    "@novu/node": "^0.21.0",
    "@typegoose/typegoose": "^11.7.0",
    "axios": "^1.6.0",
    "class-validator": "^0.14.0",
    "cloudinary": "^1.41.0",
    "cors": "^2.8.5",
    "datauri": "^4.1.0",
    "dayjs": "^1.11.10",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "express-async-errors": "^3.1.1",
    "express-list-endpoints": "^6.0.0",
    "express-list-routes": "^1.1.9",
    "express-validator": "^7.0.1",
    "google-auth-library": "^9.2.0",
    "helmet": "^7.0.0",
    "jsonwebtoken": "^9.0.2",
    "lodash": "^4.17.21",
    "moment": "^2.29.4",
    "mongoose": "7.6.3",
    "mongoose-findorcreate": "^4.0.0",
    "mongoose-id-validator2": "olawalejuwonm/mongoose-id-validator4",
    "mongoose-validation-error-message-handler": "^1.2.9",
    "morgan": "^1.10.0",
    "morgan-body": "^2.6.9",
    "multer": "^1.4.5-lts.1",
    "nodemailer": "^6.9.7",
    "nodemon": "^3.0.1",
    "openapi-types": "^12.1.3",
    "passport": "^0.6.0",
    "passport-local-mongoose": "^8.0.0",
    "pino": "^8.16.1",
    "pluralize": "^8.0.0",
    "popurelate": "^0.0.7",
    "qrcode": "^1.5.3",
    "slugify": "^1.6.6",
    "swagger-jsdoc": "^6.2.8",
    "swagger-ui-express": "^5.0.0",
    "ts-node": "^10.9.1"
  },
  "devDependencies": {
    "@faker-js/faker": "^8.2.0",
    "@types/cors": "^2.8.15",
    "@types/debug": "^4.1.10",
    "@types/express": "^4.17.20",
    "@types/jsonwebtoken": "^9.0.4",
    "@types/lodash": "^4.14.200",
    "@types/morgan": "^1.9.7",
    "@types/multer": "^1.4.9",
    "@types/node": "^20.8.10",
    "@types/nodemailer": "^6.4.13",
    "@types/pluralize": "^0.0.32",
    "@types/qrcode": "^1.5.4",
    "@types/swagger-jsdoc": "^6.0.2",
    "@types/swagger-ui-express": "^4.1.5",
    "clipboardy": "^4.0.0",
    "husky": "^8.0.3",
    "mocha": "^10.2.0",
    "prettier": "^3.0.3",
    "sucrase": "^3.34.0",
    "supertest": "^6.3.3",
    "typescript": "^5.2.2"
  }
}
